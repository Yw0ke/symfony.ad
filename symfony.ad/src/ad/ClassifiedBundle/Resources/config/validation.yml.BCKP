ad\ClassifiedBundle\Entity\Ads:
    properties:
        title:
            - NotBlank: ~
            - Type:
                type: string
                message: The value {{ value }} is not a valid {{ type }}.
            - Length:
                min: 8
                max: 50
                minMessage: "Your title must be at least {{ limit }} characters length"
                maxMessage: "Your title cannot be longer than {{ limit }} characters length"
        pic:
            - Image:
                minWidth: 800
                maxWidth: 2200
                minHeight: 600
                maxHeight: 1800
        pic1:
            - Image:
                minWidth: 800
                maxWidth: 2200
                minHeight: 600
                maxHeight: 1800
        pic2:
            - Image:
                minWidth: 800
                maxWidth: 2200
                minHeight: 600
                maxHeight: 1800
        pic3:
            - Image:
                minWidth: 800
                maxWidth: 2200
                minHeight: 600
                maxHeight: 1800
        attribute:
            - Collection:
                fields:
                    OwnerAdress:
                        - Collection:
                            fields:
                                value:
                                    - NotBlank: ~
                                    - Type:
                                        type: string
                                        message: The value {{ value }} is not a valid {{ type }}.
                                    - Length:
                                        min: 5
                allowMissingFields: "false"